---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
<title type="html">{{ site.title }}</title>
<subtitle>{{ site.description }}</subtitle>
<updated>{{ site.time | date_to_xmlschema }}</updated>
<rights>Copyright Â© 2022, {{ site.author }}</rights>
<link rel="alternate" type="text/html" href="{{ site.url }}" />
<link rel="self" type="application/atom+xml" href="{{ site.url }}{{ page.url }}" />    
<id>{{ site.url }}{{ page.url }}</id>
    
    
{% for post in site.posts %}
<entry>
    <!-- <title>{{ post.date_display }}</title> -->
    <link rel="alternate" type="text/html" href="{{ site.url }}{{ post.url }}" />
    <id>{{ site.url }}{{ post.url }}</id>
    <published>{{ post.date | date_to_xmlschema }}</published>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <author>
        <name>{{ site.author }}</name>
    </author>
    <content type="html" xml:base="{{ site.url }}{{ site.baseurl }}updates"><![CDATA[{{ post.content }}]]></content>
</entry>
{% endfor %}
</feed>